<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NHibernateIntro.Core"
                   namespace="NHibernateIntro.Core.Domain">
  <class name="Store" table="tbStore">

    <id name="StoreID" column="pk_store_id">
      <generator class="native"/>
    </id>

    <property name="Name" column="name" length="50" not-null="true"/>

    <property name="AccountingCode" column="accounting_code" length="50" not-null="false"/>

  </class>
</hibernate-mapping>